<div class="col-xs-offset-1 col-xs-10 col-sm-offset-2 col-sm-8 col-md-offset-3 col-md-6 col-lg-offset-3 col-lg-6 signin-headline">
  <h1>PUT YOUR FAKE MONEY WHERE YOUR MOUTH IS.</h1>
</div>
<div class="col-xs-offset-1 col-xs-10 col-sm-offset-3 col-sm-6 col-md-offset-4 col-md-4 col-lg-offset-4 col-lg-4">
<form action="/register/create" method="POST" id="sign-up-form" class="fs-form">

  <% if (flash && flash.err) { %>
    <ul class="alert alert-danger">
    <% Object.keys(flash.err).forEach(function(error) { %>
    <li><%- flash.err[error].message %></li>
    <% }) %>
    </ul>
  <% } %>

  <input type="text" class="form-control" placeholder="First Name" name="firstName">
  <input type="text" class="form-control" placeholder="Last Name" name="lastName">
  <input type="text" class="form-control" placeholder="Email Address" name="email">
  <input type="password" class="form-control" placeholder="Password" name="password">
  <input type="password" class="form-control" placeholder="Re-type Password" name="confirmation">
  <br/>
  <input type="submit" class="btn btn-lg btn-primary btn-block" value="SIGN UP"/>
  <input type="hidden" name="_csrf" value="<%= _csrf %>" />
</form>
</div>
<div class="signin-page">
</div>
<div class="signin-page-overlay"></div>
